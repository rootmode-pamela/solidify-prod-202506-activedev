<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Active_Loans_Company</fullName>
    <booleanFilter>1 AND (2 OR 3) AND 4</booleanFilter>
    <columns>Household_Name__c</columns>
    <columns>OPPORTUNITY.NAME</columns>
    <columns>Purpose__c</columns>
    <columns>OPPORTUNITY.STAGE_NAME</columns>
    <columns>Subject_Prop_Street__c</columns>
    <columns>Subject_Prop_St__c</columns>
    <columns>Loan_Originator__c</columns>
    <columns>Lender__c</columns>
    <columns>Loan_Program__c</columns>
    <columns>OPPORTUNITY.AMOUNT</columns>
    <columns>Date_of_Application__c</columns>
    <columns>Lock_Expiration__c</columns>
    <columns>COE__c</columns>
    <columns>Processor_Note__c</columns>
    <columns>Next_Deadline__c</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>OPPORTUNITY.STAGE_NAME</field>
        <operation>equals</operation>
        <value>Requested,Reviewed,File Started,Docs Requested,Docs Received,Qualified,Pre-approved,Application,Sent to Processing,Submitted,Approved,Clear to Close,Cleared to Close,Docs Out</value>
    </filters>
    <filters>
        <field>COE__c</field>
        <operation>notEqual</operation>
    </filters>
    <filters>
        <field>Lock_Expiration__c</field>
        <operation>notEqual</operation>
    </filters>
