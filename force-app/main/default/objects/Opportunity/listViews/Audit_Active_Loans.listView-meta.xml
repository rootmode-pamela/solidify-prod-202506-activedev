<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Audit_Active_Loans</fullName>
    <booleanFilter>(1 OR 2 OR 3) AND 4 AND 5</booleanFilter>
    <columns>OPPORTUNITY.NAME</columns>
    <columns>Purpose__c</columns>
    <columns>Subject_Prop_Street__c</columns>
    <columns>Subject_Prop_St__c</columns>
    <columns>Lender__c</columns>
    <columns>Loan_Program__c</columns>
    <columns>OPPORTUNITY.AMOUNT</columns>
    <columns>Interest_Rate__c</columns>
    <columns>Lock_Expiration__c</columns>
    <columns>COE__c</columns>
    <columns>OPPORTUNITY.STAGE_NAME</columns>
    <columns>Last_Point_Update__c</columns>
    <columns>OPPORTUNITY.LAST_ACTIVITY</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>OPPORTUNITY.STAGE_NAME</field>
        <operation>equals</operation>
        <value>Requested,Reviewed,Pre-approved,Application,Sent to Processing,Submitted,Approved,Clear to Close</value>
    </filters>
    <filters>
        <field>COE__c</field>
        <operation>notEqual</operation>
    </filters>
    <filters>
        <field>Lock_Expiration__c</field>
        <operation>notEqual</operation>
    </filters>
    <filters>
        <field>OPPORTUNITY.STAGE_NAME</field>
        <operation>notEqual</operation>
        <value>Closed Won,Archive Closed,Closed Elsewhere,Cancelled,Denied</value>
    </filters>
    <filters>
        <field>OPPORTUNITY.RECORDTYPE</field>
        <operation>equals</operation>
        <value>Opportunity.Prospecting</value>
    </filters>
    <label>Audit - Active Loans</label>
    <sharedTo>
        <role>Processor</role>
    </sharedTo>
</ListView>
