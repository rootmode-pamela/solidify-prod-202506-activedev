<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Active_Loans_Robyn</fullName>
    <booleanFilter>(1 OR 2 OR 3) AND 4 AND 5 AND 6</booleanFilter>
    <columns>Household_Name__c</columns>
    <columns>OPPORTUNITY.NAME</columns>
    <columns>Loan_Processor__c</columns>
    <columns>Loan_Originator__c</columns>
    <columns>Purpose__c</columns>
    <columns>Loan_Program__c</columns>
    <columns>Lender__c</columns>
    <columns>OPPORTUNITY.AMOUNT</columns>
    <columns>OPPORTUNITY.STAGE_NAME</columns>
    <columns>Next_Deadline__c</columns>
    <columns>Processor_Note__c</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>OPPORTUNITY.STAGE_NAME</field>
        <operation>equals</operation>
