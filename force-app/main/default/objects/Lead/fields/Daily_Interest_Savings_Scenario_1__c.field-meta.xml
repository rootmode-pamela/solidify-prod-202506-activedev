<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Daily_Interest_Savings_Scenario_1__c</fullName>
    <formula>( Current_Balance__c *  Current_Rate__c / 365) - (  First_Loan_Amount_Scenario_1__c *   Rate_Scenario_1__c / 365)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Indicates how much interest the borrower is throwing away every DAY they stay in their current loan.</inlineHelpText>
    <label>Daily Interest Savings</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
