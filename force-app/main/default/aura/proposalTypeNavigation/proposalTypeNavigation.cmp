<aura:component implements="lightning:isUrlAddressable,force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickAction,lightning:actionOverride" access="global" >
    <aura:attribute name="propRecType" type="String"/>
    <aura:attribute name="url" type="String"/>
    <aura:attribute name="pageReference" type="Object"/>
    
     <lightning:navigation aura:id="navService"/>

    <div aura:id="recordModal" role="dialog" tabindex="-1" aria-labelledby="header43" class="slds-modal slds-fade-in-open">
        <div class="slds-modal__container">
                <div class="slds-modal__header">
                    <h2 class="slds-text-heading--medium">Proposal Type</h2>
            </div>
            <div class="slds-modal__content slds-p-around--medium slds-grid slds-wrap ">   
                <lightning:button variant="brand-outline" label="Refi Analysis"  onclick="{!c.goToRefi }" />
                <lightning:button variant="brand-outline" label="Rate Analysis"  onclick="{!c.goToRateNew }" />
                <lightning:button variant="brand-outline" label="Loan Comparison"  onclick="{!c.goToScenario }" />
            </div>
            <div class="slds-modal__footer">                
                <lightning:button variant="neutral" label="Cancel" onclick="{!c.cancelDialog}" />
            </div>
        </div>
    </div>
    <div aura:id="overlay" class="slds-backdrop slds-backdrop--open"></div>


</aura:component>