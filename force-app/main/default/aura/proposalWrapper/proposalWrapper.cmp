<aura:component implements="flexipage:availableForAllPageTypes,force:hasRecordId,force:hasSObjectName">
    <aura:attribute name="isRefi" type="boolean" default="false"/>
    <aura:attribute name="recordId" type="String"/>
    <aura:attribute name="proposalRecord" type="Proposal__c"/>
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
    <force:recordData aura:id="recordLoader"
                     recordId="{!v.recordId}"
                     targetFields="{!v.proposalRecord}"
                     fields="Global_Pricing_Adjuster__c"
                     targetError="{!v.recordLoadError}"/>
   
    <lightning:outputField aura:id="test" fieldName="Global_Pricing_Adjuster__c"/>

  <c:rateProposal/> 
    
</aura:component>